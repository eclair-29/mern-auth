{
  "extends": ["airbnb", "prettier"],
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "comma-dangle": ["error", "never"],
    "no-shadow": "off",
    "no-unused-vars": ["error", { "vars": "local", "args": "none" }],
    "no-use-before-define": "off",
    "prettier/prettier": ["error"],
    "quotes": ["error", "double"]
  }
}
